{
    "name": "topthink/think-swoole",
    "description": "Swoole extend for thinkphp5.2",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "liu21st",
            "email": "liu21st@gmail.com"
        }
    ],
    "require": {
        "ext-swoole": ">=1.9.5",
        "topthink/framework": "^6.0",
        "topthink/think-helper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "think\\swoole\\": "src"
        }
    },
    "extra": {
        "think-config": {
        },
        "think": {
            "services": [
                "\\think\\swoole\\Service"
            ],
            "config": {
                "swoole": "src/config/swoole.php"
            }
        }
    },
    "require-dev": {
        "eaglewu/swoole-ide-helper": "dev-master"
    }
}
